                             (v_Vlr18601+v_Vlr18613) * v_qtd_meses;
            END IF;
            --Alterado ROD em 09.set.2009 ...(acima)

            IF v_val_rubrica > 0 AND NOT v_dat_concessaoben is null THEN
              --tdcd.extend;
              idx_cald := nvl(idx_cald, 0) + 1;

