                                  p_msgerro,
                                  p_sub_proc_erro,
                                  BEN_IDE_CLI,
                                  null);
              VI_QTD_ERROS := nvl(VI_QTD_ERROS, 0) + 1;
       END;

  END SP_GRAVA_FOLHA_PARAM;



