         and (PAR_PER_PRO >= rg.dat_ini and
             PAR_PER_PRO <=
             nvl(rg.dat_fim, to_date('01/01/2045', 'dd/mm/yyyy')));

      FOR i IN 1 .. nom_variavel.count LOOP

        IF nom_variavel(i) = COM_COL_INFORMACAO THEN
          val_variavel(i) := o_valor;
          v_val_percentual := o_valor;
          exit;
        END IF;

      END LOOP;

    EXCEPTION
