             ded_ir_doenca,
             ded_ir_pa,
             flg_pag,
             flg_ind_pago,
             flg_ind_ultimo_pag,
             tot_cred_pag,
             tot_deb_pag,
             val_liquido_pag,
             val_base_ir_pag,
             val_ir_ret_pag,
             val_base_ir_13,
             VAL_IR_13_RET,
             val_base_ir_13_pag,
             VAL_BASE_IR_13_RET_PAG,
             val_base_isencao,
             IND_PROCESSO,
             val_base_prev,
             ----- TASK 3359-----
             VAL_BASE_REDUTOR              
             )

          VALUES
            (rfol.COD_INS,
             rfol.TIP_PROCESSO,
             rfol.PER_PROCESSO,
             rfol.SEQ_PAGAMENTO,
             rfol.COD_IDE_CLI,
             rfol.COD_BENEFICIO,
             rfol.NUM_GRP,
             rfol.NUM_SEQ_BENEF,
             rfol.cod_ide_cli_ben,
             rfol.cod_ide_serv,
