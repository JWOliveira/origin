                        parc(i).tot_quota := trunc(val_parc / (v_val_dif*10/100),0)+1;
                 ELSE
                        parc(i).VAL_RUBRICA := val_parc;
                        parc(i).tot_quota := 1;
              END IF;   -- ffranco     02/2007      **********************************

    --          FOR J IN 1..NVEZES LOOP  ----->>>> R E F A Z E R LOGICA CALCULO DESCONTO > 10% DO LIQUIDO
              FOR J IN 1..parc(i).tot_quota LOOP

                  IF  J > 1  THEN
                     dat_aux  := ADD_MONTHS(PAR_PER_PRO,(J - 1));
                     parc(i).PER_PROCESSO := ADD_MONTHS(PAR_PER_PRO,(J - 1));
                     idx_caln:=nvl(idx_caln,0)+1;
                     idx_seq_detalhe := idx_seq_detalhe + 1;
                     tdcn.extend;
                     parc(i).seq_detalhe := idx_seq_detalhe;
    --                 parc(i).des_complemento := 'Parc '||j||'/'||NVEZES;
                       parc(i).des_complemento := 'Parc '||j;  -- ffranco 02/2007
                     parc(i).num_quota := J;
                  ELSE
                     parc(i).num_quota := 1;
                     parc(i).seq_detalhe := idx_seq_detalhe;
    --                 parc(i).des_complemento := 'Parc '||j||'/'||NVEZES;
    --                   parc(i).des_complemento := 'Parc '||j;  -- ffranco 02/2007
                  END IF;
                  IF j = parc(i).tot_quota  THEN   ---ffranco  02/2007
                      parc(i).val_rubrica:=val_parc - (parc(i).val_rubrica * trunc(val_parc / (v_val_dif*10/100),0));
                    ELSE
                      parc(i).val_rubrica:=parc(i).val_rubrica;
                  END IF;  --- ffranco  02/2007
                   tdcn(idx_caln):=parc(i);
              END LOOP;
          END IF;
        END IF;
        ELSE
          tdcn.extend;
          idx_caln:=nvl(idx_caln,0)+1;
          idx_seq_detalhe := idx_seq_detalhe + 1;
          parc(i).seq_detalhe := idx_seq_detalhe;
          tdcn(idx_caln):=parc(i);

        END IF;
     END LOOP;
    */
  END SP_INS_DETCAL_RET;
