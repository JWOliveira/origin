  FUNCTION SP_OBTEM_PROP_COMPOSICAO RETURN NUMBER IS
    O_VALOR NUMBER(18, 4);
    dia_ini NUMBER;
    dia_fim NUMBER;

  BEGIN
    O_VALOR := 1;

    IF COM_IND_COMP_RUB = 'N'
    --- Codic?o de inclus?o no IF de Calculo incluida por solicitac?o
    -- Paulo Miranda TT27807 o dia 16-02-2015 
    --- Inicio
    OR
       (
         --- 815002,815102,1207502,1207602 Incluidas em 17/06/2014
          COM_COD_FCRUBRICA IN (900102 ,1000102 , 808702, 805102,6500402,6500502,
                                6500702,6500802 , 815002, 815102,1207502,1207602,
                                --- Rubricas incluidas 25052015
                                805106 , 805108 , 805110
                                )
          AND COM_COD_ENTIDADE=5
          AND TO_CHAR(BEN_DAT_INICIO,'YYYYMM') <
             TO_CHAR(NVL(COM_DAT_INI_VIG,TO_DATE('01/01/2999','DD/MM/YYYY')),'YYYYMM')
       )  
    --- Termino
    
     THEN


     dia_fim:=30;
     dia_ini:=1;
     -- Condic?o alterada 2016-03-11 Data de Termino de vigencia n?o
     --- Verificaba o mes/ano de termino
     IF  COM_DAT_FIM_VIG is not null   AND 
         to_char(COM_DAT_FIM_VIG,'YYYYMM') =  to_char(PAR_PER_PRO,'YYYYMM')   
     
      THEN
        dia_fim := to_number(to_char(COM_DAT_FIM_VIG, 'dd'));
        IF COM_DAT_FIM_VIG = add_months(PAR_PER_PRO, 1) - 1 THEN
           dia_fim := 30;
        ELSE
           dia_fim := to_number(to_char(COM_DAT_FIM_VIG, 'dd'));
        END IF;
     END IF;
       ---- VALIDA??O DE INGRESSO DE RUBRICA P?S CONCESSAO --
     ---- 23-09-2016
      IF COM_DAT_INI_VIG != BEN_DAT_INICIO THEN
               IF COM_DAT_INI_VIG <= PAR_PER_PRO THEN
                dia_ini :=1;
             ELSE
                IF to_char(COM_DAT_INI_VIG,'MMYYYY')=to_char(PAR_PER_PRO,'MMYYYY')
                   THEN
                   dia_ini := to_number(to_char(COM_DAT_INI_VIG,'dd'));
                END IF;
             END IF;
      ELSE
          dia_ini :=1;
      END IF; 
     
     IF dia_fim >30 THEN
       dia_fim:=30;
     END IF;
     o_valor := (dia_fim - dia_ini + 1) / 30 ;

   END IF;

   RETURN(O_VALOR);
  END SP_OBTEM_PROP_COMPOSICAO;
