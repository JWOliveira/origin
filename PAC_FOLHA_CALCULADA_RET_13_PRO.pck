  PROCEDURE SP_OBTEM_VAL_JORN_PISO(o_valor out number) AS

  BEGIN
    BEGIN
      select jor.val_valor_percentual
        into o_valor
        from tb_valores_fatores    vf,
             tb_valores_referencia vr,
             tb_mestre_fatores     mf,
             tb_jornada            jor
       where vf.cod_ins = PAR_COD_INS
