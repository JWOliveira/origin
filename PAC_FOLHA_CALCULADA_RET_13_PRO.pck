  PROCEDURE SP_OBTEM_EVOLU_REL_FUNCIONAL AS
  begin
    begin
    -- JTS24
      SELECT EF.COD_REFERENCIA,
             EF.COD_PCCS      ,
             EF.COD_JORNADA   ,
             EF.COD_QUADRO    ,
             EF.IND_OPCAO     ,
             EF.COD_REFERENCIA_2,
             EF.COD_REFERENCIA_3,
             EF.COD_JORNADA_2   ,
             EF.COD_JORNADA_3   ,
             EF.QTD_MES_2       ,
             EF.QTD_MES_3       ,
             EF.QTD_MES
        INTO BEN_COD_REFERENCIA,
             COM_PCCS,
             COM_COD_JORNADA,
             COM_QUADRO,
             COM_OPCAO,
             COM_REFERENCIA_2,
             COM_REFERENCIA_3,
             COM_JORNADA_2   ,
             COM_JORNADA_3   ,
             COM_QTD_MES_2   ,
             COM_QTD_MES_3   ,
             COM_QTD_MES

        FROM TB_EVOLUCAO_FUNCIONAL_BEN EF
       WHERE EF.COD_INS = par_cod_ins
         AND EF.COD_IDE_CLI_SERV = BEN_IDE_CLI_SERV --Caso Pensao por morte efv 20060823
         AND EF.COD_IDE_CLI_BEN = BEN_IDE_CLI
         AND EF.COD_ENTIDADE = COM_ENTIDADE
            --Rod 01/09/09 n?o comparar cargo da Evoluc?o pois n?o muda ao longo do tempo
            --AND ((EF.COD_CARGO = COM_CARGO and PAR_TIP_PRO <> 'R') or
            --    PAR_TIP_PRO = 'R') --RAO 20060412 para n?o considerar o cargo nos retroativos
         AND EF.NUM_MATRICULA = COM_MATRICULA
         AND (PAR_PER_PRO >=
             TO_DATE('01/' || TO_CHAR(EF.DAT_INI_EFEITO, 'MM/YYYY'),
                      'DD/MM/YYYY') -- FFRANCO - 06052009
             AND
             PAR_PER_PRO <=
             NVL(EF.DAT_FIM_EFEITO, TO_DATE('01/01/2045', 'DD/MM/YYYY')))
         AND EF.FLG_STATUS = 'V'
         AND EF.COD_IDE_REL_FUNC = BEN_IDE_REL_FUNC;
    exception
      WHEN OTHERS THEN
        p_coderro       := sqlcode;
        p_sub_proc_erro := 'SP_FOLHA_CALCULADA - Evol. Func';
        p_msgerro       := 'Erro ao obter Evolucao Funcional';
        INCLUI_LOG_ERRO_FOLHA(par_cod_ins,
                              p_coderro,
                              'Calcula Folha',
                              sysdate,
                              p_msgerro,
                              p_sub_proc_erro,
                              BEN_IDE_CLI,
                              0); --efv 20060823
        --       COM_COD_FCRUBRICA); --efv 20060823
        VI_QTD_ERROS := nvl(VI_QTD_ERROS, 0) + 1;
    END;

  END SP_OBTEM_EVOLU_REL_FUNCIONAL;
