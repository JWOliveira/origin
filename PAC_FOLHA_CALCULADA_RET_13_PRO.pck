              rdcd.seq_detalhe       := idx_cald;
              rdcd.cod_fcrubrica     := '1860500';
              rdcd.seq_vig           := 1;
              rdcd.val_rubrica       := v_val_rubrica;
              rdcd.num_quota         := 1;
