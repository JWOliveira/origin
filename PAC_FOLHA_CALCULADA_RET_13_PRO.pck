              (cod_ins,tip_processo,per_processo,seq_pagamento,cod_ide_cli,cod_beneficio,
               cod_proc_grp_pag,num_seq_benef,cod_ide_cli_ben,dta_nasc,dta_ini_ben,dta_fim_ben,
