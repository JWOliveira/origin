                              COM_COD_FCRUBRICA);

        --     RAISE ERRO;
        VI_QTD_ERROS := nvl(VI_QTD_ERROS, 0) + 1;
    END;

    idx_cald := nvl(idx_cald, 0) + 1;

    rdcd.cod_ins         := PAR_COD_INS;
